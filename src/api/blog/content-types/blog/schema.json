{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "topic": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "imageURL": {
      "type": "string",
      "required": true,
      "unique": false,
      "default": "https://i.ibb.co/fY0F1Xw/stopPorn.png"
    },
    "keywords": {
      "type": "json"
    },
    "viewCount": {
      "type": "biginteger",
      "required": true,
      "default": "01"
    },
    "day": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::day.day",
      "mappedBy": "blog"
    },
    "content": {
      "type": "richtext"
    },
    "BlogId": {
      "type": "integer"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "saves_blogs"
    },
    "authorName": {
      "type": "string",
      "default": "Mohammad Shamim Reza"
    }
  }
}
